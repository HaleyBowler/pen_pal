<%= form_for(@user, action: "/users/#{@user.id}", method: :put) do |f| %>

<div class="field">
	<%= f.label :english %><br />
	<%= f.select :english, options_for_select(%w[0 1 2 3 4 5]) %>
</div>
<div class="field">
    <%= f.label :spanish %><br />
	<%= f.select :spanish, options_for_select(%w[0 1 2 3 4 5]) %>
</div>
<div class="field">
	<%= f.label :french %><br />
	<%= f.select :french, options_for_select(%w[0 1 2 3 4 5]) %>
</div>
<div class="field">
	<%= f.label :italian %><br />
	<%= f.select :italian, options_for_select(%w[0 1 2 3 4 5]) %>
</div>
<div class="field">
	<%= f.label :german %><br />
	<%= f.select :german, options_for_select(%w[0 1 2 3 4 5]) %>
</div>
<%= f.submit %>
<% end %>