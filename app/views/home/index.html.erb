<div class="jumbotron jumbotron-home">
  <div class="container home">
    <div class="page-header">
    <h1 class="home-heading">Welcome to Chatable</h1>
    <p class="home-sub">Start a chat instantly!</p>
    </div>
  </div>
</div>

<div class="container user_table">
  <div class="row">
		<% @users.each do |user| %>
		  <% if user != current_user %>
      <div class="col-sm-4">
			    <li class="random-users"><%= render partial: "display", locals: {user: user, languages: user.languages} %></li><br \>
          </div>
		  <% end %>
		<% end %>
  </div>
</div>